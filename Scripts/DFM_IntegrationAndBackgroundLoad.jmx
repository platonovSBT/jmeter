            <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="extractLocation" enabled="true">
              <stringProp name="TestPlan.comments">Сохраняем в переменную путь до скрипта и утилиты апдейта ДФО файла</stringProp>
              <stringProp name="scriptLanguage">groovy</stringProp>
              <stringProp name="parameters"></stringProp>
              <stringProp name="filename"></stringProp>
              <stringProp name="cacheKey">true</stringProp>
              <stringProp name="script">import org.apache.jmeter.services.FileServer;

String locationPackage = vars.get(&quot;lctScrpt&quot;)+File.separator+&quot;testPack&quot;+File.separator+vars.get(&quot;bankNameVtb&quot;)+&quot;E&quot;;

log.info(&quot;locationPackageVtb: &quot;+locationPackage);
vars.put(&quot;lctNcontainer&quot;, locationPackage); 
</stringProp>
            </JSR223Sampler>
