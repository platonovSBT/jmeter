            <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="clearDirectory" enabled="true">
              <stringProp name="scriptLanguage">groovy</stringProp>
              <stringProp name="parameters"></stringProp>
              <stringProp name="filename"></stringProp>
              <stringProp name="cacheKey">true</stringProp>
              <stringProp name="script">String valueLocationPackageVtb=vars.get(&quot;lctNcontainer&quot;);
File[] listFile = new File(valueLocationPackageVtb).listFiles();
for (File file : listFile) {
	if (file.getName().contains(&quot;global&quot;)) {
		File[] listDeleteFile = new File(file.getAbsolutePath()).listFiles();
		for (File delFile : listDeleteFile) {
			delFile.delete();
		}
		file.delete();
	}
}
</stringProp>
            </JSR223Sampler>
