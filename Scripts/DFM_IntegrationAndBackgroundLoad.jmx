                <stringProp name="scriptLanguage">groovy</stringProp>
                <stringProp name="parameters"></stringProp>
                <stringProp name="filename"></stringProp>
                <stringProp name="cacheKey">true</stringProp>
                <stringProp name="script">import org.apache.jmeter.services.FileServer;

StringBuilder path = new StringBuilder();
String valueLocation=vars.get(&quot;lctScrpt&quot;);
String valueBankName=vars.get(&quot;bankNameVtb&quot;);
path.append(valueLocation+File.separator+&quot;util&quot;+File.separator+&quot;rsa_&quot;+valueBankName+&quot;.txt&quot;);
File fileDel = new File(path.toString()) ;
if (fileDel.exists()){
     fileDel.delete();
 }  
FileWriter f = new FileWriter(path.toString(),true);
BufferedWriter out = new BufferedWriter(f);
out.write(vars.get(&quot;rsaKeyVtb&quot;));
out.close();
f.close();
vars.put(&quot;pathRsaKeyVtb&quot;,path.toString());</stringProp>
              </JSR223Sampler>
              <hashTree/>
              <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="copyPackageOnServer" enabled="true">
                <stringProp name="scriptLanguage">groovy</stringProp>
                <stringProp name="parameters"></stringProp>
                <stringProp name="filename"></stringProp>
                <stringProp name="cacheKey">true</stringProp>
                <stringProp name="script">import com.jcraft.jsch.Channel;
import com.jcraft.jsch.ChannelSftp;
import com.jcraft.jsch.JSch;
import com.jcraft.jsch.JSchException;
import com.jcraft.jsch.Session;
import com.jcraft.jsch.SftpException;
import com.jcraft.jsch.ChannelSftp.LsEntry;
import org.apache.commons.io.FilenameUtils;

String valuePathRsaKey=vars.get(&quot;pathRsaKeyVtb&quot;);
String valueUser=vars.get(&quot;bankNameVtb&quot;);
String valueHost=vars.get(&quot;aplOk&quot;);
StringBuilder dst=new StringBuilder();
dst.append(&quot;/home/ftpuser/ftp/&quot;+valueUser+&quot;/data/&quot;+vars.get(&quot;sessionUidVtb&quot;)+&quot;/in&quot;);
vars.put(&quot;destinationVtb&quot;,dst.toString());

StringBuilder srcPthZip=new StringBuilder();
srcPthZip.append(vars.get(&quot;lctNcontainer&quot;));
srcPthZip.append(&quot;/global&quot;);
File[] fileListDatazip = new File(srcPthZip.toString()).listFiles(); 

JSch jSch = new JSch();
jSch.addIdentity(valuePathRsaKey);
Session session = jSch.getSession(valueUser,valueHost,22);
java.util.Properties config = new java.util.Properties();
config.put(&quot;StrictHostKeyChecking&quot;, &quot;no&quot;);
config.put(&quot;PreferredAuthentications&quot;,&quot;publickey,keyboard-interactive,password&quot;);
session.setConfig(config);
session.connect();
channel = session.openChannel(&quot;sftp&quot;);
channel.connect();
channelSftp = (ChannelSftp)channel;
channelSftp.cd(dst.toString());

for (int g=0;g &lt; fileListDatazip.length;g++) {
	channelSftp.put(fileListDatazip[g].getAbsolutePath(), dst.toString()); 
	channelSftp.chmod(Integer.parseInt(&quot;7777&quot;, 8), fileListDatazip[g].getName());
}

channelSftp.ls(dst.toString());
Vector filelist=channelSftp.ls(dst.toString());

for (int i=0;i&lt;filelist.size();i++)
{
	LsEntry entry=(LsEntry)filelist.get(i);
	if (entry.getFilename().contains(&quot;control&quot;))
	{
		vars.put(&quot;sizeControlZip&quot;,Long.toString(entry.getAttrs().getSize()));
	}
	if (entry.getFilename().contains(&quot;data&quot;))
	{
		String nameVar=FilenameUtils.removeExtension(entry.getFilename());
		vars.put(nameVar,Long.toString(entry.getAttrs().getSize()));
		log.info(&quot;Имя переменной: &quot;+nameVar);
		log.info(&quot;Значение переменной: &quot;+Long.toString(entry.getAttrs().getSize()));
		
	}
}
channel.exit();
session.disconnect();
</stringProp>
