String lctUtilCpu=lctScrpt+File.separator+&quot;utilCpu.jtl&quot;;
String lctUtilMem=lctScrpt+File.separator+&quot;utilMem.jtl&quot;;
String lctUtilNet=lctScrpt+File.separator+&quot;utilNet.jtl&quot;;
String lctUtilDisk=lctScrpt+File.separator+&quot;utilDisk.jtl&quot;;
String lctUtilJmx=lctScrpt+File.separator+&quot;utilJmx.jtl&quot;;

log.info(&quot;lctScrpt: &quot;+lctScrpt);
vars.put(&quot;lctScrpt&quot;, lctScrpt); 

log.info(&quot;locationResultTest: &quot;+locationResultTest);
props.put(&quot;locationResultTest&quot;,locationResultTest);

log.info(&quot;lctPackage: &quot;+lctPackage);
vars.put(&quot;lctPackage&quot;, lctPackage); 

log.info(&quot;typeSystem: &quot;+typeSystem);
vars.put(&quot;typeSystem&quot;, typeSystem); 


log.info(&quot;lctUtilCpu: &quot;+lctUtilCpu);
props.put(&quot;lctUtilCpu&quot;,lctUtilCpu);
log.info(&quot;lctUtilMem: &quot;+lctUtilMem);
props.put(&quot;lctUtilMem&quot;,lctUtilMem);
log.info(&quot;lctUtilNet: &quot;+lctUtilNet);
props.put(&quot;lctUtilNet&quot;,lctUtilNet);
log.info(&quot;lctUtilDisk: &quot;+lctUtilDisk);
props.put(&quot;lctUtilDisk&quot;,lctUtilDisk);
log.info(&quot;lctUtilJmx: &quot;+lctUtilJmx);
props.put(&quot;lctUtilJmx&quot;,lctUtilJmx);
</stringProp>
