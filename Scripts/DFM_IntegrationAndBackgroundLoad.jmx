String valueHost=vars.get(&quot;aplOk&quot;);
StringBuilder dst=new StringBuilder();
dst.append(&quot;/home/ftpuser/ftp/&quot;+valueUser+&quot;/data/&quot;+vars.get(&quot;sessionUidVtb&quot;)+&quot;/in&quot;);
vars.put(&quot;destinationVtb&quot;,dst.toString());

