                <stringProp name="filename"></stringProp>
                <stringProp name="cacheKey">true</stringProp>
                <stringProp name="script">import com.jcraft.jsch.Channel;
import com.jcraft.jsch.ChannelSftp;
import com.jcraft.jsch.JSch;
import com.jcraft.jsch.JSchException;
import com.jcraft.jsch.Session;
import com.jcraft.jsch.SftpException;
import com.jcraft.jsch.ChannelSftp.LsEntry;

String valuePathRsaKey=vars.get(&quot;pathRsaKeyPsb&quot;);
String valueUser=vars.get(&quot;bankNamePsb&quot;);
String valueHost=vars.get(&quot;aplOk&quot;);
StringBuilder dstEd=new StringBuilder();
dstEd.append(&quot;/home/ftpuser/ftp/&quot;+valueUser+&quot;/data/&quot;+vars.get(&quot;sessionUidEdPsb&quot;)+&quot;/in&quot;);
vars.put(&quot;destinationEdPsb&quot;,dstEd.toString());

StringBuilder srcPthDataZip=new StringBuilder();
srcPthDataZip.append(vars.get(&quot;locationPackagePsb&quot;)+File.separator+&quot;2&quot;+File.separator+&quot;data_0.zip&quot;);
StringBuilder dataZipename=new StringBuilder();
dataZipename.append(dstEd.toString());
dataZipename.append(&quot;/data_0.zip&quot;);

StringBuilder srcPthControlZip=new StringBuilder();
srcPthControlZip.append(vars.get(&quot;locationPackagePsb&quot;)+File.separator+&quot;2&quot;+File.separator+&quot;control.zip&quot;);
StringBuilder dataControlname=new StringBuilder();
dataControlname.append(dstEd.toString());
dataControlname.append(&quot;/control.zip&quot;);

JSch jSch = new JSch();
jSch.addIdentity(valuePathRsaKey);
Session session = jSch.getSession(&quot;user&quot;,valueHost,22);
//Session session = jSch.getSession(valueUser,valueHost,22);
java.util.Properties config = new java.util.Properties();
config.put(&quot;StrictHostKeyChecking&quot;, &quot;no&quot;);
config.put(&quot;PreferredAuthentications&quot;,&quot;publickey,keyboard-interactive,password&quot;);
session.setConfig(config);
session.setPassword(&quot;12345678&quot;);
session.connect();
channel = session.openChannel(&quot;sftp&quot;);
channel.connect();
channelSftp = (ChannelSftp)channel;
channelSftp.cd(dstEd.toString());
channelSftp.put(srcPthDataZip.toString(), dstEd.toString()); 
channelSftp.put(srcPthControlZip.toString(), dstEd.toString()); 
channelSftp.chmod(Integer.parseInt(&quot;7777&quot;, 8), dataZipename.toString());
channelSftp.chmod(Integer.parseInt(&quot;7777&quot;, 8), dataControlname.toString());
channelSftp.ls(dstEd.toString());
Vector filelist=channelSftp.ls(dstEd.toString());
for (int i=0;i&lt;filelist.size();i++)
{
	LsEntry entry=(LsEntry)filelist.get(i);
	if (entry.getFilename().contains(&quot;control&quot;))
	{
		vars.put(&quot;sizeControlZipEditPsb&quot;,Long.toString(entry.getAttrs().getSize()));
	}
	if (entry.getFilename().contains(&quot;data_0&quot;))
	{
		vars.put(&quot;sizeDataZipEditPsb&quot;,Long.toString(entry.getAttrs().getSize()));
	}
}
channel.exit();
session.disconnect();
</stringProp>
