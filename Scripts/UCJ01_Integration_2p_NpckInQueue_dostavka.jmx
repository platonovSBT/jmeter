              <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="writeRsaKeyToFile" enabled="true">
                <stringProp name="scriptLanguage">groovy</stringProp>
                <stringProp name="parameters"></stringProp>
                <stringProp name="filename"></stringProp>
                <stringProp name="cacheKey">true</stringProp>
                <stringProp name="script">import org.apache.jmeter.services.FileServer;

StringBuilder path = new StringBuilder();
String valueLocation=vars.get(&quot;locationScriptSber&quot;);
String valueBankName=vars.get(&quot;bankNameSber&quot;);
path.append(valueLocation+File.separator+&quot;util&quot;+File.separator+&quot;rsa_&quot;+valueBankName+&quot;.txt&quot;);
File fileDel = new File(path.toString()) ;
if (fileDel.exists()){
     fileDel.delete();
 }  
FileWriter f = new FileWriter(path.toString(),true);
BufferedWriter out = new BufferedWriter(f);
out.write(vars.get(&quot;rsaKeySber&quot;));
out.close();
f.close();
vars.put(&quot;pathRsaKeySber&quot;,path.toString());</stringProp>
              </JSR223Sampler>
              <hashTree/>
              <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="copyFirstPackageOnServer" enabled="true">
                <stringProp name="scriptLanguage">groovy</stringProp>
                <stringProp name="parameters"></stringProp>
                <stringProp name="filename"></stringProp>
                <stringProp name="cacheKey">true</stringProp>
                <stringProp name="script">import com.jcraft.jsch.Channel;
import com.jcraft.jsch.ChannelSftp;
import com.jcraft.jsch.JSch;
import com.jcraft.jsch.JSchException;
import com.jcraft.jsch.Session;
import com.jcraft.jsch.SftpException;
import com.jcraft.jsch.ChannelSftp.LsEntry;


String valuePathRsaKey=vars.get(&quot;pathRsaKeySber&quot;);
String valueUser=vars.get(&quot;bankNameSber&quot;);
String valueHost=vars.get(&quot;aplOk&quot;);
StringBuilder dst=new StringBuilder();
dst.append(&quot;/home/ftpuser/ftp/&quot;+valueUser+&quot;/data/&quot;+vars.get(&quot;sessionUidSber&quot;)+&quot;/in&quot;);
vars.put(&quot;destinationSber&quot;,dst.toString());

StringBuilder srcPthDataZip=new StringBuilder();
srcPthDataZip.append(vars.get(&quot;locationPackageSber&quot;)+File.separator+&quot;1&quot;+File.separator+&quot;data_0.zip&quot;);
StringBuilder dataZipename=new StringBuilder();
dataZipename.append(dst.toString());
dataZipename.append(&quot;/data_0.zip&quot;);

StringBuilder srcPthControlZip=new StringBuilder();
srcPthControlZip.append(vars.get(&quot;locationPackageSber&quot;)+File.separator+&quot;1&quot;+File.separator+&quot;control.zip&quot;);
StringBuilder dataControlname=new StringBuilder();
dataControlname.append(dst.toString());
dataControlname.append(&quot;/control.zip&quot;);

JSch jSch = new JSch();
jSch.addIdentity(valuePathRsaKey);
Session session = jSch.getSession(&quot;user&quot;,valueHost,22);
java.util.Properties config = new java.util.Properties();
config.put(&quot;StrictHostKeyChecking&quot;, &quot;no&quot;);
config.put(&quot;PreferredAuthentications&quot;,&quot;publickey,keyboard-interactive,password&quot;);
session.setConfig(config);
session.setPassword(&quot;12345678&quot;);
session.connect();
channel = session.openChannel(&quot;sftp&quot;);
channel.connect();
channelSftp = (ChannelSftp)channel;
channelSftp.cd(dst.toString());
channelSftp.put(srcPthDataZip.toString(), dst.toString()); 
channelSftp.put(srcPthControlZip.toString(), dst.toString()); 
channelSftp.chmod(Integer.parseInt(&quot;7777&quot;, 8), dataZipename.toString());
channelSftp.chmod(Integer.parseInt(&quot;7777&quot;, 8), dataControlname.toString());
channelSftp.ls(dst.toString());
Vector filelist=channelSftp.ls(dst.toString());
for (int i=0;i&lt;filelist.size();i++)
{
	LsEntry entry=(LsEntry)filelist.get(i);
	if (entry.getFilename().contains(&quot;control&quot;))
	{
		vars.put(&quot;sizeControlZipSber&quot;,Long.toString(entry.getAttrs().getSize()));
	}
	if (entry.getFilename().contains(&quot;data_0&quot;))
	{
		vars.put(&quot;sizeDataZipSber&quot;,Long.toString(entry.getAttrs().getSize()));
	}
}
channel.exit();
session.disconnect();

</stringProp>
              </JSR223Sampler>
