                <stringProp name="filename"></stringProp>
                <stringProp name="cacheKey">true</stringProp>
                <stringProp name="script">import com.jcraft.jsch.Channel;
import com.jcraft.jsch.ChannelSftp;
import com.jcraft.jsch.JSch;
import com.jcraft.jsch.JSchException;
import com.jcraft.jsch.Session;
import com.jcraft.jsch.SftpException;


String valuePathRsaKey=vars.get(&quot;pathRsaKey&quot;);
String valueUser=vars.get(&quot;bankName&quot;);
String valueHost=vars.get(&quot;aplOk&quot;);
StringBuilder dst=new StringBuilder();
dst.append(&quot;/home/ftpuser/ftp/&quot;);
dst.append(valueUser);
dst.append(&quot;/data/&quot;);
dst.append(vars.get(&quot;sessionUid&quot;));
dst.append(&quot;/in&quot;);
vars.put(&quot;destination&quot;,dst.toString());

StringBuilder srcPthDataZip=new StringBuilder();
srcPthDataZip.append(vars.get(&quot;locationPackage&quot;));
srcPthDataZip.append(&quot;/1/data_0.zip&quot;);
StringBuilder dataZipename=new StringBuilder();
dataZipename.append(dst.toString());
dataZipename.append(&quot;/data_0.zip&quot;);

StringBuilder srcPthControlZip=new StringBuilder();
srcPthControlZip.append(vars.get(&quot;locationPackage&quot;));
srcPthControlZip.append(&quot;/1/control.zip&quot;);
StringBuilder dataControlname=new StringBuilder();
dataControlname.append(dst.toString());
dataControlname.append(&quot;/control.zip&quot;);

JSch jSch = new JSch();
jSch.addIdentity(valuePathRsaKey);
Session session = jSch.getSession(valueUser,valueHost,22);
java.util.Properties config = new java.util.Properties();
config.put(&quot;StrictHostKeyChecking&quot;, &quot;no&quot;);

session.setConfig(config);
session.connect();
channel = session.openChannel(&quot;sftp&quot;);
channel.connect();
channelSftp = (ChannelSftp)channel;
channelSftp.cd(dst.toString());
channelSftp.put(srcPthDataZip.toString(), dst.toString()); 
channelSftp.put(srcPthControlZip.toString(), dst.toString()); 
channelSftp.chmod(Integer.parseInt(&quot;7777&quot;, 8), dataZipename.toString());
channelSftp.chmod(Integer.parseInt(&quot;7777&quot;, 8), dataControlname.toString());</stringProp>
